<UserControl x:Class="Incubator_2.Forms.Tasks.TasksMain"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Incubator_2.Forms.Tasks" xmlns:oneinstance="clr-namespace:Incubator_2.Forms.OneInstance"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <GroupBox Style="{DynamicResource MainGroupBox}" Header="ЗАДАЧИ В РАБОТЕ" Grid.Column="0">
            <oneinstance:NoContent/>
        </GroupBox>
        <GridSplitter Grid.Column="0" Width="5" Background="Transparent" ResizeDirection="Columns"/>
        <GroupBox Style="{DynamicResource MainGroupBox}" Header="ВЫПОЛНЕННЫЕ ЗАДАЧИ" Grid.Column="1">
            <oneinstance:NoContent/>
        </GroupBox>
    </Grid>
</UserControl>
