<UserControl x:Class="Incubator_2.Forms.Communication.CommunicationMain"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Incubator_2.Forms.Communication"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <GroupBox Style="{DynamicResource MainGroupBox}" Header="ВНУТРЕННЯЯ СВЯЗЬ">
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition MinWidth="100"/>
                <ColumnDefinition Width="2*" MinWidth="300" />
            </Grid.ColumnDefinitions>
            <StackPanel/>
            <Border Grid.Column="1" CornerRadius="5" Background="{DynamicResource Black}">
                
            </Border>
        </Grid>
    </GroupBox>
</UserControl>
