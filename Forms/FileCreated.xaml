<UserControl x:Class="Incubator_2.Forms.FileCreated"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Incubator_2.Forms" xmlns:fa="http://schemas.fontawesome.io/icons/"
             mc:Ignorable="d" 
             d:DesignHeight="45" d:DesignWidth="400">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="40"/>
            <ColumnDefinition Width="33*"/>
            <ColumnDefinition Width="40"/>
        </Grid.ColumnDefinitions>
        <CheckBox Style="{DynamicResource Selector}" Grid.RowSpan="2"/>
        <Label Style="{DynamicResource LabelElement}" Content="Имя не определено" Grid.Column="1" VerticalAlignment="Top" Grid.RowSpan="2" FontSize="11" VerticalContentAlignment="Bottom"/>
        <fa:FontAwesome Style="{DynamicResource IconButton}" Grid.Column="2" Icon="Remove" Margin="0,11,0,0" Grid.RowSpan="2" VerticalAlignment="Top" />
        <Label Style="{DynamicResource LabelElement}" Content="00:00:00 12.12.2012" Grid.Row="1" Grid.Column="1" FontSize="11" VerticalAlignment="Top"/>
    </Grid>
</UserControl>
