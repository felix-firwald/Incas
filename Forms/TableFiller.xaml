<UserControl x:Class="Incubator_2.Forms.TableFiller"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Incubator_2.Forms"
             mc:Ignorable="d" 
             d:DesignHeight="80" d:DesignWidth="800">
    <Expander x:Name="ExpanderMain" Margin="5" Style="{DynamicResource ExpanderMain}">
        <Grid MinHeight="80">
            <DataGrid Style="{DynamicResource DataGridMain}" ItemsSource="{Binding Grid}"/>
        </Grid>
    </Expander>
    
</UserControl>
