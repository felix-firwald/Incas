<Window x:Class="Incubator_2.Windows.PreviewWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Incubator_2.Windows"
        mc:Ignorable="d"
        Title="Предпросмотр" Height="720" Width="1080" Style="{DynamicResource WindowBase}" ShowInTaskbar="False" Topmost="True" WindowStartupLocation="CenterScreen" Closed="OnClosed">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>
        <DocumentViewer x:Name="Preview" Grid.Row="1" Background="{DynamicResource Basic}" />
    </Grid>
</Window>
