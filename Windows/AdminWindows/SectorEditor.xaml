<Window x:Class="Incubator_2.Windows.AdminWindows.SectorEditor"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Incubator_2.Windows.AdminWindows"
        mc:Ignorable="d"
        Title="SectorEditor" Height="80" Width="400" Style="{DynamicResource Dialog}" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="40"/>
            <RowDefinition Height="40"/>
        </Grid.RowDefinitions>
        <TextBox Style="{DynamicResource TextBoxMain}" Tag="Название сектора" Text="{Binding SectorName}"/>
        <Button Style="{DynamicResource ButtonMain}" Content="Сохранить сектор" Grid.Row="1" Click="Button_Click"/>
    </Grid>
</Window>
