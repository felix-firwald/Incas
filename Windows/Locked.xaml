<Window x:Class="Incubator_2.Windows.Locked"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Incubator_2.Windows" xmlns:fa="http://schemas.fontawesome.io/icons/"
        mc:Ignorable="d"
        Title="Locked" Height="350" Width="400" Style="{DynamicResource Dialog}" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="2"/>
            <RowDefinition Height="40"/>
        </Grid.RowDefinitions>
        <fa:FontAwesome Icon="Lock" HorizontalAlignment="Center" VerticalAlignment="Bottom" Foreground="{DynamicResource Icon}" FontSize="36"/>
        <Label Style="{DynamicResource LabelElement}" Content="Данное рабочее пространство заблокировано" HorizontalAlignment="Center" Margin="0,171,0,0" Grid.RowSpan="3" />
        <Rectangle Fill="{DynamicResource Element}" Grid.Row="2"/>
        <Button Style="{DynamicResource ButtonMain}" Grid.Row="3" Content="Вернуться к окну входа" Click="Button_Click"/>
    </Grid>
</Window>
