<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--Цвета фона и текста-->
    <SolidColorBrush x:Key="Black" Color="#1d1d1f"/>
    <SolidColorBrush x:Key="LightBlack" Color="#252528"/>
    <SolidColorBrush x:Key="Dark" Color="#2d2d30"/>
    <SolidColorBrush x:Key="Basic" Color="#2f3239"/>
    <SolidColorBrush x:Key="Area" Color="#393c43"/>
    <SolidColorBrush x:Key="Focus" Color="#3E4148"/>
    <SolidColorBrush x:Key="Element" Color="#43464d"/>
    <SolidColorBrush x:Key="DarkLight" Color="#7D8089"/>
    <SolidColorBrush x:Key="GrayLight" Color="#A7ABB7"/>
    <SolidColorBrush x:Key="Light" Color="#D3D5DB"/>
    <!--Цвета исключительно для контролов-->
    <SolidColorBrush x:Key="Error" Color="#FF0033"/>
    <SolidColorBrush x:Key="DarkPurple" Color="#6600BA"/>
    <SolidColorBrush x:Key="Purple" Color="#8B00FF"/>
    <SolidColorBrush x:Key="LightPurple" Color="#B255FF"/>
    <SolidColorBrush x:Key="DarkYellow" Color="#F19A0B"/>
    <SolidColorBrush x:Key="Yellow" Color="#F5A623"/>
    <SolidColorBrush x:Key="LightYellow" Color="#F7BD5B"/>
</ResourceDictionary>